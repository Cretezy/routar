{
  "name": "routex",
  "version": "0.0.15",
  "description": "Modern Node router",
  "keywords": [
    "router",
    "routex"
  ],
  "homepage": "https://routex.netlify.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/routexjs/routex"
  },
  "license": "MIT",
  "author": "Charles Crete <charles@cretezy.com>",
  "files": [
    "dist"
  ],
  "main": "dist/lib/index.js",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "format": "prettier --write '**/*.{js,ts,md,json}'",
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "watch": "tsc -w",
    "prepublishOnly": "run-s format lint test build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    }
  },
  "dependencies": {
    "@types/node": "^12.0.2",
    "parseurl": "^1.3.3",
    "path-to-regexp": "^3.0.0",
    "qs": "^6.7.0",
    "throng": "^4.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/parseurl": "^1.3.1",
    "@types/path-to-regexp": "^1.7.0",
    "@types/qs": "^6.5.3",
    "@types/supertest": "^2.0.7",
    "@types/throng": "^4.0.2",
    "@types/uuid": "^3.4.4",
    "codecov": "^3.2.0",
    "husky": "^2.3.0",
    "jest": "^24.5.0",
    "jest-config": "^24.5.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
